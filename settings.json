{
  "files.associations": {
  "**/*.html": "html",
  "**/templates/**/*.html": "django-html",
  "**/templates/**/*": "django-txt",
  "**/requirements{/**,*}.{txt,in}": "pip-requirements"
  },
  "emmet.includeLanguages": {
    "django-html": "html"
  },
  "python.pythonPath": "venv/bin/python",
  "beautify.language": {
    "html": [
        "htm",
        "html",
        "django-html"
    ]
  },
  "prettier.disableLanguages":["django-html"],
}